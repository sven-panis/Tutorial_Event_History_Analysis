@article{panisAnalyzingResponseTimes2020,
  title = {Analyzing {{Response Times}} and {{Other Types}} of {{Time-to-Event Data Using Event History Analysis}}: {{A Tool}} for {{Mental Chronometry}} and {{Cognitive Psychophysiology}}},
